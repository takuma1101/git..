 <%= form_with model: @address, url: public_address_path, local: true do |f|%>
    <table>
        <tr>
        <div calss="form-group">
          <th><%= f.label :"郵便番号(ハイフンなし)" %></th>
          <td><%= f.text_field :postal_code%></td>
        </div>
        </tr>
        <tr>
        <div calss="form-group">
          <th><%= f.label :"住所" %></th>
          <td><%= f.text_field :address %></td>
        </div>
        </tr>
        <tr>
        <div calss="form-group">
          <th><%= f.label :"宛名" %></th>
          <td><%= f.text_field :name%></td>
        　<td><%= f.submit "変更内容を保存", class:"btn btn-success" %></td>
        </div>
        </tr>
      </table>
  <%end%>
